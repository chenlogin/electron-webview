{
  "name": "mini-app",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "./dist/main/main.min.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config webpack/electron.webpack.js --watch",
    "package": "electron-builder"
  },
  "build": {
    "productName": "ElectronApp",
    "appId": "com.sirius.miniapp",
    "copyright": "meng",
    "directories": {
      "output": "build"
    },
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://xxxxx/"
      }
    ],
    "files": [
      "!node_modules/**/*",
      "package.json",
      "src/**",
      "pages/**",
      "dist/**",
      "node_modules/electron-screenshots/**/*",
      "node_modules/electron-tabs/**/*",
      "node_modules/loudness/**/*"
    ],
    "mac": {
      "icon": "./assets/logo.icns"
    },
    "win": {
      "icon": "./assets/logo.ico",
      "target": "portable"
    }
  },
  "repository": "https://github.com/chenlogin/webview-electron",
  "keywords": [
    "Electron",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "electron-screenshots": "0.0.16",
    "electron-tabs": "^0.15.0",
    "loudness": "^0.4.1",
    "node-disk-info": "^1.3.0",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2"
  }
}
